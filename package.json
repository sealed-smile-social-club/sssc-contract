  },
  "homepage": "https://github.com/sealed-smile-social-club/sssc-contractt#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@truffle/hdwallet-provider": "^2.0.6",
    "ethers": "^5.6.5",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31"
  },
  "devDependencies": {
    "@remix-project/remixd": "^0.5.7",
    "@types/jest": "^28.1.2",
    "@types/node": "^14.11.2",
    "dotenv-cli": "^5.1.0",
    "jest": "^28.1.1",
    "supertest": "^6.2.3",
    "truffle": "^5.5.10",
    "truffle-flattener": "^1.6.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
